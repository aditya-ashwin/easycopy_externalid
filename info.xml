  </classloader>
  <civix>
    <namespace>CRM/EasycopyExternalid</namespace>
    <format>22.12.1</format>
    <angularModule>crmEasycopyExternalid</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
</extension>
